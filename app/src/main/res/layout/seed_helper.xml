            android:text="24" />

        <Button
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:fontFamily="@font/jalnan"
            android:background="@drawable/roundbtnviolet"
            android:text="36" />

        <Button
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_weight="1"
            android:fontFamily="@font/jalnan"
